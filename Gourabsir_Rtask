pmiss <- function(vec)
{
round(sum(is.na(vec)/lenght(vec)*100,2))
}

pmiss <- function(vec)
{
nmiss =sum(is.na(vec))
permiss =nmiss/length(vec)*100
return(permiss)
}

x=c(2,5,NA,8)
y=c(NA,NA,2.25,NA)
pmiss(x)
pmiss(y)

pmiss <- function(vec)
{
return(round(sum(is.na(vec))/lenght(vec)*100,2))}

apply(fram,2,pmiss)

percent_missing2 <- function(data)
{
return(apply(fram,2,pmiss))
}


