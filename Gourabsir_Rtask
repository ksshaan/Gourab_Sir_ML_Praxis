#precent_missing(data) -> precentage of missing values in each columns of df
is.na(mtcars)

View(is.na(mtcars))

sum(is.na(mtcars))# it gives zero it adds all false.. results in 0
apply(is.na(mtcars),2,sum)# apply to all coumns
apply(is.na(mtcars),2,sum)/nrow(mtcars)*100

percent_missing <- function(data)
{
return(apply(is.na(data),2,sum)/nrow(data)*100)
}

percent_missing(mtcars)

fram<- read.csv('framingham.csv')
percent_missing(fram)

